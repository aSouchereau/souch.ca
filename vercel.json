{
  "rewrites": [
    {
      "source": "/apiv1/:path*",
      "has": [
        {
          "type": "header",
          "key": "Authorization",
          "value": "Bearer REDACTED"
        },
        {
          "type": "header",
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "type": "header",
          "key": "Accept",
          "value": "application/json"
        }
      ],
      "destination": "REDACTED"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}